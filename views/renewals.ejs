<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/renewalStyles.css" type="text/css">
    <title>Renewals</title>
</head>
<body>
    <div class="header"><h1> &#9733 </h1></div>
    <section class="renewalSection">
        <h1>Explore Upcoming Renewals</h1>
        <ul>
                <% users.forEach(user =>{%>
                    <li>
                        <h2><%= user.email %></h2>
                        <h3><%= user.name %></h3>
                        <a href="/renewals">Contact Now</a>
                    </li>
               <%})%>
        </ul>
    </section>
   
</body>
</html>